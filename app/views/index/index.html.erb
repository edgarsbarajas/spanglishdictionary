<div>
  <ul class="index-list">
    <% @words.each do |word| %>
      <li>
        <h2>
          <%= word.word %>
        </h2>
        <p><%= word.definition %></p>
        <p class="example"><%= word.example %></p>
        <span>by <%= word.user.username %></span>
      </li>
    <% end %>
  </ul>
</div>